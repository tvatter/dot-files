{
  "settingsSync.keybindingsPerPlatform": false,
  "editor.wordWrap": "bounded",
  "editor.formatOnSave": true,
  "editor.formatOnSaveMode": "modificationsIfAvailable",
  "editor.tabSize": 2,
  "editor.insertSpaces": true,
  "editor.detectIndentation": false,
  "editor.cursorStyle": "line",
  "editor.lineNumbers": "on",
  "editor.wordSeparators": "/\\()\"':,.;<>~!@#$%^&*|+=[]{}`?-",
  "editor.suggestSelection": "first",
  "editor.inlineSuggest.enabled": true,
  "explorer.confirmDragAndDrop": false,
  "explorer.confirmDelete": false,
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "workbench.colorTheme": "Material Theme Darker High Contrast",
  "workbench.iconTheme": "eq-material-theme-icons-darker",
  "workbench.editorAssociations": {
    "*.ipynb": "jupyter-notebook"
  },
  "workbench.editor.enablePreview": false,
  "workbench.startupEditor": "none",
  "terminal.integrated.inheritEnv": true,
  "terminal.integrated.enableMultiLinePasteWarning": "never",
  // "vim.easymotion": true,
  // "vim.incsearch": true,
  // "vim.useSystemClipboard": true,
  // "vim.useCtrlKeys": true,
  // "vim.hlsearch": true,
  // "vim.leader": "<space>",
  // // "aws.profile": "profile:tvatter",
  // // "terminal.integrated.env.linux": {
  // //    "DISPLAY":"localhost:10.0"
  // //  },
  "latex-workshop.view.pdf.viewer": "tab",
  "latex-workshop.synctex.afterBuild.enabled": true,
  "[latex]": {
    "editor.defaultFormatter": "nickfode.latex-formatter",
    "editor.formatOnSave": false
  },
  "python.languageServer": "Pylance",
  "[python]": {
    "editor.codeActionsOnSave": {
      "source.organizeImports": "explicit",
      "source.fixAll": "explicit"
    },
    "editor.defaultFormatter": "charliermarsh.ruff"
  },
  "ruff.importStrategy": "fromEnvironment",
  "mypy-type-checker.importStrategy": "fromEnvironment",
  "mypy-type-checker.showNotifications": "always",
  "mypy-type-checker.reportingScope": "workspace",
  "mypy-type-checker.preferDaemon": true,
  "jupyter.askForKernelRestart": false,
  "jupyter.interactiveWindow.creationMode": "perFile",
  "notebook.formatOnSave.enabled": true,
  "notebook.codeActionsOnSave": {
    "source.fixAll": true,
    "source.organizeImports": true
  },
  "notebook.cellToolbarLocation": {
    "default": "right",
    "jupyter-notebook": "left"
  },
  "notebook.lineNumbers": "on",
  "notebook.output.textLineLimit": 80,
  "notebook.output.scrolling": true,
  "notebook.diff.ignoreOutputs": true,
  "[r]": {
    "editor.wordSeparators": "/\\()\"':,.;<>~!@#$%^&*|+=[]{}`?-"
  },
  // "r.bracketedPaste": true,
  // "r.sessionWatcher": true,
  "git.autofetch": true,
  "git.suggestSmartCommit": false,
  "git.confirmSync": false,
  "security.workspace.trust.untrustedFiles": "open",
  "files.associations": {
    "*.qmd": "rmd"
  },
  "[markdown]": {
    "editor.defaultFormatter": "DavidAnson.vscode-markdownlint"
  },
  "markdownlint.config": {
    "default": true,
    "MD003": {
      "style": "atx"
    },
    "MD007": {
      "indent": 4
    },
    "no-hard-tabs": false
  },
  "github.copilot.advanced": {},
  "github.copilot.enable": {
    "quarto": false,
    "*": true
  },
  "github.copilot.editor.enableAutoCompletions": true,
  "cmake.configureOnOpen": true,
  "cmake.showOptionsMovedNotification": false,
  "cmake.pinnedCommands": [
    "workbench.action.tasks.configureTaskRunner",
    "workbench.action.tasks.runTask"
  ],
  "cSpell.language": "en,fr,fr-FR",
  "cSpell.enableFiletypes": [
    "quarto"
  ],
  "cSpell.userWords": [
    "jupyter",
    "revealjs",
  ],
  "[cpp]": {
    "editor.wordBasedSuggestions": "off",
    "editor.suggest.insertMode": "replace",
    "editor.semanticHighlighting.enabled": true
  },
  "remote.SSH.defaultExtensions": [
    "charliermarsh.ruff",
    "chrischinchilla.vscode-pandoc",
    "cschlosser.doxdocgen",
    "davidanson.vscode-markdownlint",
    "eamodio.gitlens",
    "equinusocio.vsc-material-theme",
    "equinusocio.vsc-material-theme-icons",
    "github.copilot",
    "github.copilot-chat",
    "github.vscode-github-actions",
    "github.vscode-pull-request-github",
    "huizhou.githd",
    "james-yu.latex-workshop",
    "jeff-hykin.better-cpp-syntax",
    "lacroixdavid1.vscode-format-context-menu",
    "ms-azuretools.vscode-docker",
    "ms-python.debugpy",
    "ms-python.mypy-type-checker",
    "ms-python.python",
    "ms-python.vscode-pylance",
    "ms-toolsai.jupyter",
    "ms-toolsai.jupyter-keymap",
    "ms-toolsai.jupyter-renderers",
    "ms-toolsai.vscode-jupyter-cell-tags",
    "ms-toolsai.vscode-jupyter-slideshow",
    "ms-vscode-remote.remote-containers",
    "ms-vscode-remote.remote-ssh",
    "ms-vscode-remote.remote-ssh-edit",
    "ms-vscode-remote.remote-wsl",
    "ms-vscode.cmake-tools",
    "ms-vscode.cpptools",
    "ms-vscode.cpptools-extension-pack",
    "ms-vscode.cpptools-themes",
    "ms-vscode.makefile-tools",
    "ms-vscode.remote-explorer",
    "ms-vsliveshare.vsliveshare",
    "nickfode.latex-formatter",
    "pkief.material-icon-theme",
    "pomdtr.excalidraw-editor",
    "quarto.quarto",
    "reditorsupport.r",
    "tamasfe.even-better-toml",
    "torn4dom4n.latex-support",
    "twxs.cmake",
    "valentjn.vscode-ltex",
    "visualstudioexptteam.intellicode-api-usage-examples",
    "visualstudioexptteam.vscodeintellicode",
    "vscodevim.vim"
  ]
  // "yapf.args": [
